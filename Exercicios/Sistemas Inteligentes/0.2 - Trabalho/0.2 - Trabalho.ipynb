    "    def Verificar_Estados(self):\n",
    "        if (len(self.objArrayEstados) == 0):\n",
    "            print('Nenhum estado foi encontrado')\n",
    "            self.reset()\n",
    "            self.declare(Fact(action='restart'))\n",
    "            self.run()\n",
    "\n",
    "\n",
    "    @Rule(Fact(action='restart'))\n",
