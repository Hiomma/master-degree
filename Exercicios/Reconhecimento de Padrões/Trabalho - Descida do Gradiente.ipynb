    "\r\n",
    "\r\n",
    "for nr_Index in range(100):\r\n",
    "    x = x - nr_Step * np.polyval(derivative, x)\r\n",
    "\r\n",
    "print(x)\r\n",
    "\r\n",
    "print(function)\r\n",
    "print(derivative)"
